    "## Count Digits"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "230c5877",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2023-10-19T09:22:01.611668Z",
     "start_time": "2023-10-19T09:22:01.595972Z"
    }
   },
   "outputs": [],
   "source": [
    "def countDigits(n: int) -> int:\n",
    "    count = 0\n",
    "    for i in str(n):\n",
    "        if i == '0':\n",
    "            continue\n",
    "        elif n % int(i) == 0:\n",
    "            count += 1\n",
    "    return count\n",
    "        "
   ]
